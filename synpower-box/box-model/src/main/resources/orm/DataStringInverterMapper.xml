	<select id="getTimesPower" parameterType="Map"
		resultMap="BaseResultMap">
		SELECT sum(active_power) active_power,data_time from(
		SELECT DISTINCT
		device_id,data_time,active_power from data_string_inverter
		where
		data_time>=#{minTime} and #{maxTime}>=data_time
		<foreach collection="devices" item="deviceId"
			open=" and device_id IN (" close=")" separator=",">
			#{deviceId}
		</foreach>
		)a
		GROUP BY data_time
	</select>
