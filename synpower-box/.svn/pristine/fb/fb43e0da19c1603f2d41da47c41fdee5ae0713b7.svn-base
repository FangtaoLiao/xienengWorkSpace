	<properties>
		<aspectj.version>1.7.1</aspectj.version>
		<java.version>1.8</java.version>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<roo.version>1.3.1.RC2</roo.version>
		<slf4j.version>1.6.4</slf4j.version>
